{
    "name": "photo-dir-to-json",
    "version": "1.2.4",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/danallan/photo-dir-to-json.git"
    },
    "scripts": {
        "build:docs": "api-extractor run --local --verbose && api-documenter markdown --input-folder temp --output-folder docs",
        "build": "yarn test:ci && tsc && yarn build:docs",
        "clean": "rm -r dist",
        "release": "release-it",
        "lint": "eslint . --max-warnings 0",
        "format": "prettier . --write",
        "format:check": "prettier . --check",
        "test": "vitest",
        "test:ci": "yarn lint && yarn format:check && vitest run --coverage",
        "postinstall": "husky",
        "prepack": "pinst --disable",
        "postpack": "pinst --enable"
    },
    "dependencies": {
        "date-fns": "^4.1.0",
        "exifreader": "^4.32.0",
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "@commitlint/cli": "^20.1.0",
        "@commitlint/config-conventional": "^20.0.0",
        "@microsoft/api-documenter": "^7.28.0",
        "@microsoft/api-extractor": "^7.55.0",
        "@release-it/conventional-changelog": "^10.0.2",
        "@tsconfig/node20": "^20.1.8",
        "@types/eslint": "^9.6.1",
        "@types/node": "^24.10.1",
        "@vitest/coverage-v8": "^4.0.12",
        "eslint": "^9.39.1",
        "eslint-config-prettier": "^10.1.8",
        "esrun": "^3.2.30",
        "husky": "^9.1.7",
        "pinst": "^3.0.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.6",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.47.0",
        "vitest": "^4.0.12"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "type": "module",
    "packageManager": "yarn@4.5.3",
    "resolutions": {
        "conventional-changelog-conventionalcommits": "9.1.0"
    }
}
