{
    "name": "photo-dir-to-json",
    "version": "1.0.2",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/danallan/photo-dir-to-json.git"
    },
    "scripts": {
        "build:docs": "api-extractor run --local --verbose && api-documenter markdown --input-folder temp --output-folder docs",
        "build": "yarn test:ci && tsc && yarn build:docs",
        "clean": "rm -r dist",
        "release": "release-it",
        "test": "vitest",
        "test:ci": "vitest run",
        "coverage": "vitest run --coverage"
    },
    "dependencies": {
        "date-fns": "^3.4.0",
        "exifreader": "^4.21.1",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@microsoft/api-documenter": "^7.23.37",
        "@microsoft/api-extractor": "^7.42.3",
        "@release-it/conventional-changelog": "^8.0.1",
        "@tsconfig/node20": "^20.1.2",
        "@types/node": "^20.11.27",
        "@vitest/coverage-v8": "^1.3.1",
        "esrun": "^3.2.26",
        "release-it": "^17.1.1",
        "typescript": "^5.4.2",
        "vitest": "^1.3.1"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "type": "module",
    "packageManager": "yarn@4.1.1"
}
